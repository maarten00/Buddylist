<% @postings.each do |posting|
  if posting.kind_of? Blogposting %>
        <%= render partial: "shared/blogposting", locals: {show_username: TRUE, blogposting: posting} %>
    <% elsif posting.kind_of? Fileposting %>
        <%= render partial: "shared/fileposting", locals: {show_username: TRUE, fileposting: posting} %>
    <% end
end %>