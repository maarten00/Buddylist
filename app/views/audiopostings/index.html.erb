<h1>Audiopostings</h1>

<div class="tag_cloud row-fluid span12">
  <% tag_cloud Audioposting.tag_counts, %w[s m l] do |tag, css_class| %>
      <%= link_to tag.name, audiotag_path(tag.name), class: css_class %>
  <% end %>
</div>

<%= render partial: "shared/audiopostingslist", locals: {:show_username => TRUE} %>
